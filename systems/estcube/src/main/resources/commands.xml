<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframew ork.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
       http://www.springframework.org/schema/beans 
       http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
       http://www.springframework.org/schema/context 
       http://www.springframework.org/schema/context/spring-context-2.5.xsd
       http://camel.apache.org/schema/spring 
       http://camel.apache.org/schema/spring/camel-spring.xsd">

	<bean id="limit1" class="org.hbird.exchange.configurator.LimitComponentRequest">
		<constructor-arg index="0" value="Lower"/>
		<constructor-arg index="1" value="Parameter3"/>
		<constructor-arg index="2" value="9.6"/>
		<constructor-arg index="3" value="Double"/>
		<constructor-arg index="4" value="TestParameter3LowerLimit"/>
		<constructor-arg index="5" value="The lower limit of the Test Parameter3"/>
	</bean>

	<bean id="monitoring" class="org.hbird.exchange.configurator.SystemMonitoringComponentRequest"/>

	<bean id="parameterstore" class="org.hbird.exchange.configurator.ParameterArchiveComponentRequest"/>
	<bean id="commandstore" class="org.hbird.exchange.configurator.CommandArchiveComponentRequest"/>
	<bean id="taskexecutor" class="org.hbird.exchange.configurator.TaskExecutorComponentRequest"/>
			
	<bean id="parameter10" class="org.hbird.exchange.configurator.ScriptComponentRequest">
		<constructor-arg index="0" value=""/>
		<constructor-arg index="1" value="Parameter 10 Evaluator"/>
		<constructor-arg index="2" value="This script component calculates parameter 10"/>
		<constructor-arg index="3" value="// name=&quot;Parameter10&quot; description=&quot;A test script parameter&quot; unit=&quot;Volt&quot; input=&quot;Parameter1&quot; input=&quot;Parameter2&quot; &#13; var Parameter10=Parameter1*10 + Parameter2;"/>
	</bean>
	
	<bean id="commands" class="org.hbird.exchange.configurator.CommandComponentRequest"/>
	
	<bean id="components" class="java.util.ArrayList">
		<constructor-arg>
        	<list>
            	<ref bean="limit1" />
 	          	<ref bean="monitoring" />
 	          	<ref bean="parameterstore" />
 	          	<ref bean="commandstore" />
 	          	<ref bean="parameter10" />
 	          	<ref bean="commands" />
  	          	<ref bean="taskexecutor" />
         	</list>
	    </constructor-arg>
	</bean>
	
</beans>